<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <block class="Aimsinfosoft\Base\Block\Adminhtml\System\Config\InformationBlocks\Basic" name="aminfotab.basic">
            <action method="setClass">
                <argument name="class" xsi:type="string">aimsinfosoft-info-block</argument>
            </action>
            <block class="Aimsinfosoft\Base\Block\Adminhtml\System\Config\InformationBlocks\VersionInfo" name="aminfotab.version"/>
            <block class="Aimsinfosoft\Base\Block\Adminhtml\System\Config\InformationBlocks\ExistingConflicts" name="aminfotab.conflicts"/>
            <block class="Aimsinfosoft\Base\Block\Adminhtml\System\Config\InformationBlocks\Basic" name="aminfotab.buttons.wrapper">
                <action method="setClass">
                    <argument name="class" xsi:type="string">aims-buttons-container</argument>
                </action>
                <block class="Aimsinfosoft\Base\Block\Adminhtml\System\Config\InformationBlocks\FeatureRequest" name="aminfotab.feature"/>
                <block class="Aimsinfosoft\Base\Block\Adminhtml\System\Config\InformationBlocks\UserGuide" name="aminfotab.guide"/>
            </block>
        </block>
    </body>
</page>
