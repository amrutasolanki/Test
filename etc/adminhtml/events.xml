<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="Aimsinfosoft_base_add_information_content">
        <observer name="Aimsinfosoft_base_add_information_content" instance="Aimsinfosoft\Base\Observer\GenerateInformationTab"/>
    </event>

    <event name="controller_action_predispatch">
        <observer name="Aimsinfosoft_base_admin_notification"
                  instance="Aimsinfosoft\Base\Observer\PreDispatchAdminActionController" />
    </event>

    <event name="admin_system_config_save">
        <observer name="Aimsinfosoft_base_config_save" instance="Aimsinfosoft\Base\Observer\SaveConfig"/>
    </event>
</config>
